attachButton.MouseButton1Click:Connect(function()
	status.Text = "Sorry, you're not sigma ðŸ˜”"
end)

status.Parent = main
status.BackgroundTransparency = 1
status.Position = UDim2.new(0, 0, 0.93, 0)
